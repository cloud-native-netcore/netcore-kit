{
  "API_VERSION": "1.0",
  "SERVICE_VERSION": "0.0.1",
  "QualifiedAssemblyPattern": "NetCoreKit.Samples.*",
  "Hosts": {
    "BasePath": "/",
    "Externals": {
      "CurrentUri": "http://localhost:32501"
    }
  },
  "Features": {
    "EfCore": {
      "MySqlDb": {
        "Database": "maindb",
        "UserName": "root",
        "Password": "letmein",
        "DbInfo": "5.7.14-mysql",
        "ConnString": "server={0};port={1};uid={2};pwd={3};database={4}"
      },
      "PostgresDb": {
        "FQDN": "<dbhost>:<port>",
        "Database": "<database>",
        "UserName": "postgres",
        "Password": "postgres",
        "ConnString": "Host={0};Port={1};User ID={2};Password={3};Database={4}"
      }
    },
    "CleanArch": { "Enabled": true },
    "ApiVersion": { "Enabled": true },
    "OpenApi": {
      "OpenApiUI": { "Enabled": true },
      "Profiler": { "Enabled": true },
      "ApiInfo": {
        "Title": "Todo APIs",
        "Description": "An application with Swagger, Swashbuckle, and API versioning.",
        "ContactName": "Vietnam Devs",
        "ContactEmail": "thangchung.onthenet@gmail.com",
        "TermOfService": "Shareware",
        "LicenseName": "MIT",
        "LicenseUrl": "https://github.com/cloudnative-netcore/netcorekit/blob/master/LICENSE"
      }
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information"
      }
    }
  },
  "AllowedHosts": "*"
}
